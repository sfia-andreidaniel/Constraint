@UI_Form DemoForm {
	width: 400;
	height: 400;

	left: 10;
	top: 10;

	caption: 'Demo form';

	@UI_TabsPanel Tabs1 {
		left: 20;
		top: 20;
		right: 20;
		bottom: 40;

		@UI_Tab Tab1 {

			caption: 'Tab 1';


			@UI_Button Btn1 {
				caption: 'Hello world';
				bottom: center;
				left: center;
			};

			@UI_CheckBox Check1 {
				caption: 'I''m a checkbox';
				left: 20;
				top: 20;
				right: Btn1 right 0;
			};

			@UI_RadioBox Radio1 {
				caption: 'I''m a radiobox';
				left: 20;
				top: Check1 bottom 20;
				right: Btn1 right 0;
			};

			@UI_RadioBox Radio2 {
				caption: 'I''m a radiobox too';
				left: 20;
				top: Radio1 bottom 20;
				right: Btn1 right 0;
			};

			@UI_RadioBox Radio3 {
				caption: 'I''m a disabled radiobox';
				left: 20;
				top: Radio2 bottom 20;
				right: Btn1 right 0;

				disabled: true;
				value: true;
			};

			@UI_ListBox List1 {
				left: Radio3 right 20;
				top: 20;
				bottom: Btn1 top 20;
				right: 20;
			};

		};

		@UI_Tab Tab2 {
			caption: 'Tab 2';
		};
		
	};


};
